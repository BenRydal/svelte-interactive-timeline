{
  "name": "svelte-interactive-timeline",
  "version": "0.1.0",
  "description": "A canvas-based interactive timeline component for Svelte 5 with zoom, pan, and pluggable rendering layers",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenRydal/svelte-interactive-timeline.git"
  },
  "homepage": "https://github.com/BenRydal/svelte-interactive-timeline#readme",
  "bugs": {
    "url": "https://github.com/BenRydal/svelte-interactive-timeline/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "daisyui": "^5.0.0",
    "publint": "^0.2.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.0"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "timeline",
    "canvas",
    "p5",
    "visualization"
  ],
  "license": "MIT"
}
